mydata<-read.csv("H1full.csv")
attach(mydata)
#Fixed Effects (within) and Random Effects (random)
library(plm)
plmwithin <- plm(ADR~LeadTime, data = mydata, model = "within")
plmrandom <- plm(ADR~LeadTime, data = mydata, model = "random")
summary(plmwithin)
summary(plmrandom)
#Effect of time on internet usage
fixef(plmwithin,type="dmean")
summary(fixef(plmwithin, type = "dmean"))
#Extracting fixed effects with fixef
twoway <- plm(ADR~LeadTime,data=mydata,model="within",effect="time")
fixef(twoway,effect="time")
